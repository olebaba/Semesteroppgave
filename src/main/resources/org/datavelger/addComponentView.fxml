                                  <TableColumn fx:id="pricecol" onEditCommit="#changePriceCellEvent" prefWidth="75.0" text="Pris" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
