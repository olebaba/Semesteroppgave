                        <ChoiceBox fx:id="componentBox" layoutX="136.0" layoutY="286.0" prefWidth="175.0" style="-fx-border-color: #919e65;" />
                        <TextField fx:id="txt_name" layoutX="136.0" layoutY="336.0" prefHeight="36.0" prefWidth="175.0" promptText="Navn" style="-fx-border-color: #919e65;" />
