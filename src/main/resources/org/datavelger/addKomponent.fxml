<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="556.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.datavelger.KomponentController">
   <children>
      <TableView fx:id="table" layoutX="20.0" layoutY="14.0" prefHeight="269.0" prefWidth="542.0">
        <columns>
          <TableColumn fx:id="namecol" prefWidth="75.0" text="Navn" />
          <TableColumn fx:id="pricecol" prefWidth="75.0" text="Pris" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="txt_name" layoutX="61.0" layoutY="362.0" promptText="Navn" />
      <TextField fx:id="txt_price" layoutX="61.0" layoutY="416.0" promptText="Pris" />
      <Button fx:id="btn_add" layoutX="83.0" layoutY="465.0" mnemonicParsing="false" onAction="#AddRecord" text="Legg til" />
      <Button fx:id="btnLoadAll" layoutX="422.0" layoutY="362.0" mnemonicParsing="false" text="Last inn alle bestillinger" />
      <Label fx:id="labInfo" layoutX="425.0" layoutY="406.0" prefHeight="18.0" prefWidth="134.0" />
   </children>
</AnchorPane>
