                            <String fx:value="Pris"></String>
                            <String fx:value="Fjern filtrering"></String>
                        </ChoiceBox>
                        <Button fx:id="filterBtn" layoutX="577.0" layoutY="431.0" mnemonicParsing="false" onAction="#filtering" prefWidth="175.0" style="-fx-border-color: #919e65;" text="Filtrer" />
